
{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "YallaLondonStagingS3Access",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:PutObjectAcl"
      ],
      "Resource": [
        "arn:aws:s3:::yalla-london-staging/*"
      ]
    },
    {
      "Sid": "YallaLondonStagingS3ListBucket",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": [
        "arn:aws:s3:::yalla-london-staging"
      ]
    },
    {
      "Sid": "YallaLondonStagingS3MultipartUpload",
      "Effect": "Allow",
      "Action": [
        "s3:AbortMultipartUpload",
        "s3:ListMultipartUploadParts",
        "s3:ListBucketMultipartUploads"
      ],
      "Resource": [
        "arn:aws:s3:::yalla-london-staging",
        "arn:aws:s3:::yalla-london-staging/*"
      ]
    }
  ]
}
